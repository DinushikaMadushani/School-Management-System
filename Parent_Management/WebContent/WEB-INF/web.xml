<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

<servlet>
	<servlet-name>AdminServlet</servlet-name>
	<servlet-class>com.parent.AdminServlet</servlet-class>	
</servlet> 

<servlet-mapping>
	<servlet-name>AdminServlet</servlet-name>
	<url-pattern>/add</url-pattern>	
</servlet-mapping> 

<servlet>
	<servlet-name>interfaceServlet</servlet-name>
	<servlet-class>com.parent.interfaceServlet</servlet-class>	
</servlet> 

<servlet-mapping>
	<servlet-name>interfaceServlet</servlet-name>
	<url-pattern>/in</url-pattern>	
</servlet-mapping> 


<servlet>
	<servlet-name>ParentLogin</servlet-name>
	<servlet-class>com.parent.ParentLogin</servlet-class>	
</servlet> 

<servlet-mapping>
	<servlet-name>ParentLogin</servlet-name>
	<url-pattern>/log</url-pattern>	
</servlet-mapping> 

<servlet>
	<servlet-name>UpdateParentServlet</servlet-name>
	<servlet-class>com.parent.UpdateParentServlet</servlet-class>	
</servlet> 

<servlet-mapping>
	<servlet-name>UpdateParentServlet</servlet-name>
	<url-pattern>/update</url-pattern>	
</servlet-mapping>    
                  
<servlet>
	<servlet-name>DeleteParentServlet</servlet-name>
	<servlet-class>com.parent.DeleteParentServlet</servlet-class>	
</servlet> 

<servlet-mapping>
	<servlet-name>DeleteParentServlet</servlet-name>
	<url-pattern>/delete</url-pattern>	
</servlet-mapping>    
</web-app>